{
  "description": "tablimit",
  "homepage_url": "https://github.com/raidoh/max-tabs-limit-warning",
  "manifest_version": 2,
  "name": "tablimit - fork of max-tab-limit",
  "permissions": ["tabs", "notifications", "storage"],
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "32": "tab.svg"
  },
  "browser_action": {
    "browser_style": true,
    "default_title": "Max Tabs Limit",
    "default_icon": {
      "32": "tab.svg"
    }
  },
  "version": "1.23",
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "57.0a1"
    }
  },
  "options_ui": {
    "page": "options.html",
    "browser_style": true,
    "chrome_style": true
  }
}
